<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lzq.demo.mapper.PaymentsMapper">

    <resultMap id="BaseResultMap" type="com.lzq.demo.domain.Payments">
            <id property="paymentId" column="payment_id" jdbcType="BIGINT"/>
            <result property="orderId" column="order_id" jdbcType="BIGINT"/>
            <result property="amount" column="amount" jdbcType="DECIMAL"/>
            <result property="paymentTime" column="payment_time" jdbcType="TIMESTAMP"/>
            <result property="paymentMethod" column="payment_method" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        payment_id,order_id,amount,
        payment_time,payment_method
    </sql>
</mapper>
